{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "address",
        "phone_num",
        "provider_id"
    ],
    "properties": {
        "address": {
            "type": "string",
            "default": "",
            "title": "The address Schema",
            "examples": [
                "8 Mercier Ct"
            ]
        },
        "phone_num": {
            "type": "integer",
            "default": 0,
            "title": "The phone_num Schema",
            "examples": [
                12345667890
            ]
        },
        "provider_id": {
            "type": "integer",
            "default": 0,
            "title": "The provider_id Schema",
            "examples": [
                1
            ]
        }
    },
    "examples": [{
        "address": "8 Mercier Ct",
        "phone_num": 12345667890,
        "provider_id": 1
    }]
}